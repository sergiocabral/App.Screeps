{
  "name": "app.screeps",
  "version": "1.0.0",
  "description": "Implementation of Screeps game by sergiocabral.com",
  "main": "output/main.js",
  "scripts": {
    "push-official": "rollup -c rollup.config.js --environment auth:screeps.auth.official.screeps.json",
    "push-gohorse": "rollup -c rollup.config.js --environment auth:screeps.auth.gohorse.screeps.json",
    "push-screepsplus": "rollup -c rollup.config.js --environment auth:screeps.auth.screepsplus.screeps.json",
    "watch-official": "rollup -cw rollup.config.js --environment auth:screeps.auth.official.screeps.json",
    "watch-gohorse": "rollup -cw rollup.config.js --environment auth:screeps.auth.gohorse.screeps.json",
    "watch-screepsplus": "rollup -cw rollup.config.js --environment auth:screeps.auth.screepsplus.screeps.json",
    "test": "jest --passWithNoTests",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"output/**/*.js\"",
    "lint": "eslint \"src/**/*.ts\"",
    "format+lint": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiocabral/App.Screeps.git"
  },
  "author": "sergiocabral.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sergiocabral/App.Screeps/issues"
  },
  "homepage": "https://github.com/sergiocabral/App.Screeps#readme",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@sergiocabral/helper": "^1.29.3",
    "@sergiocabral/screeps": "^1.6.4",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.173",
    "@types/screeps": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "rollup": "^2.56.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
